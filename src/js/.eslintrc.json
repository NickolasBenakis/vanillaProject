{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "Atomics": "readable",
        "SharedArrayBuffer": "readable"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-unreachable": "error",
        "indent": [
            "error",
            4
        ]
    }
}